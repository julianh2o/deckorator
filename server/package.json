{
  "name": "deckorator_server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --setupFiles dotenv/config",
    "dev": "APP_DIR=`pwd` tsx watch src",
    "codegen:schema": "graphql-codegen --config=codegen.schema.yaml",
    "codegen:gql": "gqlg --schemaFilePath ./src/generated/all.graphql --destDirPath ./src/generated/gql --depthLimit 2 --includeCrossReferences",
    "codegen:hooks": "graphql-codegen --config=codegen.hooks.yaml",
    "codegen": "npm run codegen:schema && npm run codegen:gql && npm run codegen:hooks",
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "async-retry": "^1.3.3",
    "axios": "^1.6.7",
    "bullmq": "^5.3.1",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "file-type": "^19.0.0",
    "lodash-es": "^4.17.21",
    "minio": "^7.1.3",
    "openai": "^4.28.0",
    "p-iteration": "^1.1.8",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@types/async-retry": "^1.4.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.19",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "gql-generator": "^2.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}